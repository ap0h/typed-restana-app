{
  "name": "typed-restana-app",
  "version": "1.0.0",
  "description": "Type-safe Restana service with Zod validation and OpenAPI generation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": " ts-node-dev --respawn --transpile-only examples/server.ts",
    "start": "node lib/examples/server.js",
    "generate-openapi": "tsx src/scripts/generate-typed-openapi.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm run build",
    "example:basic": "tsx examples/simple-usage.ts",
    "example:server": "tsx examples/server.ts"
  },
  "keywords": [
    "restana",
    "typescript",
    "zod",
    "validation",
    "openapi",
    "type-safe",
    "api",
    "rest"
  ],
  "author": "Nemanja Stankovic <neca.rxrx@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "restana": "^4.9.0",
    "zod": "^3.24.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    },
    "restana": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@zod/core": "^0.11.6",
    "cors": "^2.8.5",
    "eslint": "^8.54.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "restana": "^4.9.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.2",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}